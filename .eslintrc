{
  "env": {
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "warn",
    "no-var": "warn",
    "no-const-assign": "error",
    "prefer-const": "warn",
    "complexity": "warn",
    "eqeqeq": "warn",
    "require-await": "warn"
  }
}
